	<!-- reglas de navegacion para el flujo de alumnos -->

	<navigation-rule>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>menuPrincipal.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>fail</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>menuPrincipal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>students</from-outcome>
			<to-view-id>menuAlumnos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>teachers</from-outcome>
			<to-view-id>menuInstructor.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>menuAlumnos.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>make</from-outcome>
			<to-view-id>realizarPagoAlumnos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>search</from-outcome>
			<to-view-id>buscarPagoAlumnos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>menuPrincipal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>realizarPagoAlumnos.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>existing</from-outcome>
			<to-view-id>pagoAlumnoExistente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>pagoAlumnoNuevo.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>menuAlumnos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>buscarPagoAlumnos.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>student</from-outcome>
			<to-view-id>pagosPorAlumno.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>last</from-outcome>
			<to-view-id>ultimosPagos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>menuAlumnos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>pagoAlumnoExistente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>realizarPagoAlumnos.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>send</from-outcome>
			<to-view-id>enviarCorreo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>enviarCorreo.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>pagoAlumnoExistente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>pagoAlumnoNuevo.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>send</from-outcome>
			<to-view-id>enviarCorreoAN.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>realizarPagoAlumnos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>enviarCorreoAN.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>pagoAlumnoNuevo.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>ultimosPagos.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>buscarPagoAlumnos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>pagosPorAlumno.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>consult</from-outcome>
			<to-view-id>historialPagosAlumno.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>buscarPagoAlumnos.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>historialPagosAlumno.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>send</from-outcome>
			<to-view-id>enviarCorreoPPA.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>pagosPorAlumno.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>enviarCorreoPPA.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>historialPagosAlumno.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<!-- reglas de navegacion para el flujo de maestros -->

	<navigation-rule>
		<from-view-id>menuInstructor.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>existing</from-outcome>
			<to-view-id>menuInstructorExistente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>new</from-outcome>
			<to-view-id>pagoInstructorNuevo1.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>menuPrincipal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>menuInstructorExistente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>make</from-outcome>
			<to-view-id>pagoInstructorExistente.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>serch</from-outcome>
			<to-view-id>busquedaPagoInstructor.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>menuInstructor.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>pagoInstructorExistente.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>menuInstructorExistente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>busquedaPagoInstructor.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>menuInstructorExistente.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>pagoInstructorNuevo1.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>next</from-outcome>
			<to-view-id>pagoInstructorNuevo2.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>menuInstructor.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>pagoInstructorNuevo2.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>back</from-outcome>
			<to-view-id>pagoInstructorNuevo1.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

</faces-config>
